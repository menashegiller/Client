@charset "utf-8";
/* ScssuserForm */

.ng-invalid:not(form)  {
    border-right: 5px solid #a94442;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    float: right;
    padding-right: 1px;
}

/*.ng-valid{
  background-color: white;
}*/

.Logout, .LogoutBtn, .accordionSave, .accordionSend {
  width: 100%;
  float: left;
}

.UserAccountForm {
  width: 100%;
  float: left;
  color: #FFFFFF;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  clear: left;
}

.NameUser {
  width: 100%;
  margin-bottom: 15px;
  margin-top: 30px;
  float: left;
  overflow: hidden;
  a {
    color: #FFFFFF;
  }
  p {
    direction: rtl;
  }
}

.AboutuserForm {
  width: 100%;
  float: right;
  margin-bottom: 30px;
  h1, p {
    width: 100%;
    float: right;
    margin-bottom: 10px;
  }
  h1 {
    font-size: 2.7em;
    font-weight: bold;
  }
  p {
    font-size: 1.5em;
  }
}

.userFormInstructions {
  width: 100%;
  float: right;
  margin-bottom: 30px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  h2 {
    font-weight: bold;
    font-size: 1.5em;
  }
  h3 {
    font-weight: bold;
  }
}

.RemarkTitle {
  width: 100%;
  float: right;
  font-weight: bold;
  margin-bottom: 5px;
}

.accordionInputBox {
  width: 100%;
  float: right;
}

.RegUserBoxsLine {
  margin-bottom: 40px;
}

/*----------------------------------- Upload File Process -------------------------*/

.DocreqContainer, .DocreqSelect, .ProcessIcon, .DocreqUpLoudBoxLine {
  width: 100%;
  float: right;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.DocreqContainer p {
  padding-bottom: 5px;
}

.DocreqSelect {
  max-width: 60px;
}

.EmptyIcon, .ChackIcon {
  font-size: 2.5em;
  float: right;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  cursor: pointer;
}

.EmptyIcon, .ChackIcon {
  display: none;
}

.SelctedChack {
  display: block;
}

.UpLoudFileBtnBox {
  width: 90%;
  float: right;
  margin-top: 10px;
}

.DocreqUpLoudBoxLine {
  border-bottom: 2px solid #DCDCDC;
  padding-bottom: 25px;
  margin-bottom: 25px;
}

.FileNotUpLoad {
  float: right;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 20px;
  margin-right: 20px;
}

.ProcessBox {
  width: 100%;
  height: 50px;
  float: left;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #E6E6E6;
  border: 1px solid #BFBFBF;
  border-radius: 5px;
  margin-top: 20px;
  display: none;
}

.ProcessIcon {
  float: left;
  font-size: 2.5em;
  max-width: 40px;
  padding-top: 4px;
  margin-left: 5px;
  cursor: pointer;
}

.ProcessColor {
  width: 50%;
  float: right;
  height: 43px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #004467;
  border-radius: 5px;
  margin: 2px;
  display: block;
}

/*----------------------------------- WorkingProcess Selcted -------------------------*/

.SelctedBtn {
  background-color: #E6E6E6 !important;
  &:hover {
    background-color: #E6E6E6 !important;
  }
}

.PersonaLetter {
  width: 100%;
  float: right;
}

.PersonaLetterList {
  counter-reset: subsection;
  list-style-type: none;
}

.PersonaLetterTitle::before {
  counter-increment: subsection;
  content: counters(subsection, ".") ".";
  margin-left: 5px;
}

.WarrCercheckbox {
  width: 50px;
  float: right;
  cursor: pointer;
}

.WarrCercheTitle {
  width: 80%;
  margin-right: 10px;
  margin-bottom: 0px;
}

.dismiss {
  display: none;
}

/*----------------------------------- Military -------------------------*/

.MilitaryRadioBox {
  width: 100%;
  float: right;
  height: 50px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.PermanentDateBox {
  display: none;
}

.SaderDateBox {
  display: block;
}

.WarrCercheBox {
  padding-top: 20px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: block;
}

.Warrcheck {
  width: 100%;
  float: right;
  margin-bottom: 10px;
  margin-top: 10px;
}

.MilitaryRadioBox {
  background-color: #F0F0F0;
  border-radius: 10px;
}

.MilitaryRadioCon {
  width: 100%;
  max-width: 500px;
  float: right;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 10px;
  padding-right: 15px;
  overflow: hidden;
  margin-left: auto;
  margin-right: auto;
}

.MilTitle {
  float: right;
  margin-right: 10px;
}

.MilBox {
  width: 21%;
  float: right;
  margin-left: 13px;
}

.RadioBtn {
  width: 25px;
  float: right;
  height: 30px;
}
.EconomicBox, .DeclarationBox {
  width: 100%;
  float: right;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  margin-bottom: 10px;
}
.EconomicBoxList {
  width: 60%;
  float: right;
 }
 .EconomicBoxList p{
  width: 100%;
  float: right;
  margin-bottom: 5px;
 }
.DeclarationBox {
  width: 100%;
  float: right;
}

.RadioTitle {
  float: right;
  margin-right: 15px;
  margin-left: 15px;
}

.EconomicRadio {
  width: 145px;
  float: right;
}

.EconomicRadioBox {
  width: 130px;
  float: right;
  margin-top: 20px;
}

.EconomicPar {
  width: 61%;
  float: right;
}

.DeclarationPar {
  width: 84%;
  float: right;
  padding-top: 5px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

.DeclarationTitle {
  padding-top: 30px;
  margin-bottom: 0px;
}

@media only screen and (min-width: 700px) {
  .UserAccountForm {
    padding-top: 20px;
  }
  .NameUser {
    width: 60%;
    float: right;
    margin-bottom: 0px;
    margin-top: 0px;
  }
  .Logout {
    max-width: 195px;
    float: left;
    padding-top: 4px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }
  .Warrcheck {
    width: 47%;
    margin-left: 30px;
  }
  .WarrCercheTitle {
    width: 70%;
  }
  .MilitaryRadioCon {
    padding-right: 20px;
  }

  .InputBoxBginDate {
	width: 100% !important;
	margin-left: 0px !important;
   }

  .MoreInputBoxBig {
    width: 100% !important;
   }

}

@media only screen and (min-width: 750px) {
  .accordionInputBox {
    width: 47%;
    margin-left: 30px;
  }
  .InputBaginDate, .InputEndDate, .EconomicPlaceBox {
    width: 100%;
  }
  .InputBoxBig {
    width: 100% !important;
    margin-left: 0px !important;
  }
  .InputBoxDate, .InputBoxStatos {
    width: 100%;
    margin-left: 0px;
  }
  /*----------------------------------- Military -------------------------*/
  .TypeServiceBox {
    width: 100% !important;
  }
  .WarrCercheBox {
    width: 100% !important;
    padding-top: 20px;
  }
  .Warrcheck {
    width: 47%;
    margin-left: 30px;
  }
  .EconomicRadio {
    margin-bottom: 23px;
  }
  .EconomicRadioBox {
    margin-top: 0px;
  }
}

@media only screen and (min-width: 900px) {
  .UserAccountForm {
    width: 375px;
  }
  .accordionSave {
    margin-right: 30px;
    max-width: 285px;
  }
  .accordionSend {
    max-width: 285px;
    margin-left: 10px;
  }
}

@media only screen and (min-width: 950px) {
  .accordionInputBox {
    width: 48%;
  }
  .EconomicPlaceBox {
    width: 100%;
  }
  .InputBoxStatos {
    float: left;
  }
  .InputBaginDate, .InputEndDate {
    width: 30% !important;
    margin-left: 39px;
  }
  .InputTuitionAmount {
    width: 30% !important;
    margin-left: 39px;
    float: left;
  }
  /*----------------------------------- Upload File Process -------------------------*/
  .DocreqUpLoudBoxLine {
    width: 93%;
    margin-right: 58px;
  }
  /*----------------------------------- WorkingProcess Selcted -------------------------*/
  .WorkingProcess {
    max-width: 56.94%;
  }
  .UpLoudFileBoxSlected {
    width: 36%;
  }
}

@media only screen and (min-width: 970px) {
  .EconomicPlaceBox {
    width: 48.19%;
  }
}

@media only screen and (min-width: 1000px) {
  .UserAccountForm {
    padding-top: 73px;
  }
  .EconomicRadio {
    margin-bottom: 0px;
  }
}

@media only screen and (min-width: 1100px) {
  .DeclarationPar {
    width: 94%;
  }
}

@media only screen and (min-width: 1125px) {
  .Warrcheck {
    width: 47%;
    margin-bottom: 0px;
  }
  .WarrCercheTitle {
    width: 70%;
  }
}

@media only screen and (min-width: 1200px) {
  /*----------------------------------- Military -------------------------*/
  .WarrCercheBox {
    padding-top: 47px;
  }
  .Warrcheck {
    margin-top: 0px;
  }
  .MilitaryRadioCon {
    padding-right: 38px;
    float: right;
    max-width: 50%;
  }
  .MilBox {
    width: 22%;
  }
}

@media only screen and (min-width: 1250px) {
  .UserAccountForm {
    width: 500px;
    padding-top: 94px;
  }
  .InputBaginDate, .InputEndDate, .InputTuitionAmount {
    width: 31% !important;
  }
  /*----------------------------------- WorkingProcess Selcted -------------------------*/
  .WorkingProcess {
    max-width: 64.94%;
  }
  .UpLoudFileBoxSlected {
    width: 28%;
  }
}

@media only screen and (min-width: 1350px) {
  .accordionInputBox {
    width: 48%;
  }
  .InputBoxDate, .InputBoxStatos {
    width: 48.71%;
  }
  /*----------------------------------- WorkingProcess Selcted -------------------------*/
  .WorkingProcess {
    max-width: 66.94%;
  }
  .UpLoudFileBoxSlected {
    width: 26%;
  }
}